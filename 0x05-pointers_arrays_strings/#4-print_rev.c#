#include "main.h"
#include <stdio.h>
/**
 * print_rev  - print a string in reverse follow by a new line
 * @s : string variable
 * Return: nothing
 */

void print_rev(char *s)
{
	int c = 0;
	int i;

	while (*(str + c) != '\0')
	{
		putchar(*(str + c));
		c = c + 1;
	}
	while (c >= 0)

	putchar('\n');
}
